<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="no.hig.hers.ludoclient.ClientTempUIController">
   <top>
      <Button fx:id="buttonSpill" mnemonicParsing="false" onAction="#goToSpill" text="Spill" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button fx:id="buttonChat" mnemonicParsing="false" onAction="#goToChat" text="Chat" BorderPane.alignment="CENTER" />
   </bottom>
   <left>
      <Button fx:id="buttonMain" mnemonicParsing="false" onAction="#goToMain" text="Main" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
